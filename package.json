{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "react-ssr",
  "main": "./dist/app.js",
  "scripts": {
    "dev": "npm run dev:client & npm run dev:server & nodemon",
    "dev:client": "NODE_ENV=dev webpack --config webpack.client.js",
    "dev:server": "NODE_ENV=dev webpack --config webpack.server.js",
    "prd": "npm run prd:client && npm run prd:server && pm2 restart ./dist/app.js",
    "prd:client": "NODE_ENV=prd webpack --config webpack.client.js",
    "prd:server": "NODE_ENV=prd webpack --config webpack.server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flypig95/react-ssr.git"
  },
  "keywords": [
    "react",
    "ssr",
    "express",
    "webpack"
  ],
  "author": "chencong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flypig95/react-ssr/issues"
  },
  "homepage": "https://github.com/flypig95/react-ssr#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
